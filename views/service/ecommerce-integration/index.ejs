      padding: 50px 0;
    }

    .testimonial-author {
      flex-direction: column;
      align-items: flex-start !important;
    }

    .testimonial-author .author-avatar {
      margin-bottom: 10px;
    }
  }
  .scroll-up.active-scroll {
    display: none;
  }
