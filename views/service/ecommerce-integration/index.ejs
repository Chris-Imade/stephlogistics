<style>
  :root {
    --primary: #2a9d8f;
    --secondary: #062e39;
    --light: #f8f9fa;
    --dark: #343a40;
    --white: #ffffff;
    --gray: #6c757d;
    --light-gray: #e9ecef;
  }

  body {
    font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif;
    color: #333;
    line-height: 1.6;
  }

  h1,
  h2,
  h3,
  h4,
  h5,
  h6 {
    font-weight: 700;
    color: var(--secondary);
  }

  .section-padding {
    padding: 80px 0;
  }

  /* Header & Navigation */
  .breadcrumb-area {
    background: linear-gradient(
      135deg,
      var(--secondary) 0%,
      var(--primary) 100%
    );
    padding: 60px 0;
    color: var(--white);
    padding-top: 10rem;
  }

  .breadcrumb-inner h1 {
    color: var(--white);
    font-size: 2.8rem;
    margin-bottom: 15px;
  }

  .breadcrumb {
    justify-content: center;
    background: transparent;
  }

  .breadcrumb-item a {
    color: rgba(255, 255, 255, 0.8);
    text-decoration: none;
  }

  .breadcrumb-item.active {
    color: var(--white);
  }

  .breadcrumb-item + .breadcrumb-item::before {
    color: rgba(255, 255, 255, 0.6);
  }

  /* Main Content Styling */
  .service-details-content img {
    border-radius: 8px;
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
  }

  .service-details-content .title {
    margin: 25px 0 15px;
  }

  .service-details-content p {
    margin-bottom: 20px;
    font-size: 1.05rem;
  }

  /* Benefits Section */
  .benefit-item {
    padding: 20px;
    border-radius: 8px;
    background-color: var(--white);
    box-shadow: 0 3px 10px rgba(0, 0, 0, 0.08);
    height: 100%;
    transition: transform 0.3s ease;
    margin-bottom: 15px;
  }

  .benefit-item:hover {
    transform: translateY(-5px);
  }

  .benefit-item .icon {
    color: var(--primary);
  }

  .benefit-item h4 {
    font-size: 1.25rem;
    margin-bottom: 10px;
  }

  /* Process Steps */
  .process-step {
    position: relative;
  }

  .process-step:not(:last-child)::after {
    content: "";
    position: absolute;
    top: 40px;
    left: 20px;
    width: 1px;
    height: calc(100% - 20px);
    background-color: var(--primary);
  }

  .step-number span {
    font-weight: bold;
  }

  /* Platform Logos */
  .platform-logo {
    transition: transform 0.3s ease;
    margin-bottom: 15px;
    padding: 15px;
    background-color: var(--white);
    border-radius: 8px;
    box-shadow: 0 3px 10px rgba(0, 0, 0, 0.05);
  }

  .platform-logo:hover {
    transform: translateY(-5px);
  }

  .platform-logo img {
    height: 60px;
    object-fit: contain;
    margin-bottom: 10px;
  }

  .platform-logo p {
    font-weight: 600;
  }

  /* Testimonials */
  .testimonial-item {
    border-left: 4px solid var(--primary);
    transition: transform 0.3s ease;
  }

  .testimonial-item:hover {
    transform: translateY(-5px);
  }

  .testimonial-content p {
    font-size: 1.1rem;
  }

  /* Call to Action */
  .service-details-cta {
    background: linear-gradient(
      135deg,
      var(--primary) 0%,
      var(--secondary) 100%
    );
  }

  .service-details-cta .btn-light:hover {
    background-color: var(--secondary);
    color: var(--white);
  }

  /* Sidebar Styling */
