<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
    <title><%= title %></title>

    <!-- Meta and styles start -->
    <%- include ../partials/payment-meta %>
    <%- include ../partials/payment-styles %>
    <!-- Meta and styles end -->

  </head>
  <body>
    <!-- Start Site Preloader -->
    <div id="trucker__preloader">
      <div id="trucker__circle_loader"></div>
      <div class="trucker__loader_logo">
        <img src="/assets/images/logo/logo.png" alt="Preload" />
      </div>
    </div>
    <!-- End Site Preloader -->

    <!-- Start Scroll To Top Here  -->
    <div class="scroll-up">
      <svg
        class="scroll-circle svg-content"
        width="100%"
        height="100%"
        viewBox="-1 -1 102 102"
      >
        <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
      </svg>
    </div>
    <!-- End Scroll To Top Here -->

    <!-- Header -->
    <%- include ../partials/navbar %>

    <!-- Main Content -->
    <main><%- body %></main>

    <!-- Footer -->
    <%- include ../partials/footer %>

    <!-- Start Overlay Add -->
    <div class="trucker__overlay"></div>
    <!-- End Overlay Add -->

    <!--Start Js Files-->
    <%- include ../partials/payment-script %>
    <!-- End Js File -->
  </body>
</html>
